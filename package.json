{
  "name": "express-router-controller-khmer",
  "version": "1.3.3",
  "license": "xaio-din",
  "keywords": [
    "routing-controller",
    "express-router",
    "express-router-controller",
    "express-router-controller-khmer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tuydenna/express-router-controller.git"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "start": "concurrently \"npx tsc --watch\" \"npx nodemon -q ./build/server.js\"",
    "build": "tsc && tsc-alias",
    "build:pro": "npx tsc --project tsconfig-npm-pubish.json && tsc-alias",
    "publish": "npm publish",
    "test": "node ./build/test/server.js",
    "watch": "tsc && (concurrently \"npx tsc --watch\" \" tsc-alias -w\" \"npx nodemon -q ./build/test/server.js\")"
  },
  "dependencies": {
    "express": "~4.16.1",
    "reflect-metadata": "^0.1.13",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.3",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "tsc-alias": "^1.8.16"
  }
}
